
#include<bits/stdc++.h>
using namespace std;
int fab(int n)
{
    if(n==1)
    {
        return 0;
    }
    if(n==2)
    {
        return 1;
    }
    return fab(n-1)+fab(n-2);
}
int main()
{
    int n;
    cout<<"enter n to get nth fabonacci number ";
    cin>>n;
    cout<<endl<<n<<"th fabonacci number is "<<fab(n);
}